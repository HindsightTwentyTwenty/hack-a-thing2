{
  "manifest_version": 2,
  "name": "iBrowse",
  "version": "0.1",
  "permissions": [
    "tabs"
  ],
	"content_scripts": [
	  {
	    "matches": [
	      "<all_urls>"
	    ],
	    "js": ["star.js"]
	  }
	],
	"browser_action": {
	  "default_icon": "icon.png"
    /*"default_popup": "starContainer.html"*/
	},
	"background": {
	  "scripts": ["background.js"]
	}
}
